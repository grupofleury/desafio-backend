{
  "version": "2.4.2",
  "compilerOptions": {
      "lib": ["es5", "es6"],
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "outDir": "./app/tsc",                        /* Redirect output structure to the directory. */
      "rootDir": "./",
  },
  "exclude": [
      "node_modules"
  ]
}